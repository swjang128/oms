{{>plugin/header}}

<!-- ================== BEGIN page-css ================== -->
<link href="/assets/plugins/datatables.net-bs5/css/dataTables.bootstrap5.min.css" rel="stylesheet" />
<link href="/assets/plugins/datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css" rel="stylesheet" />
<link href="/assets/plugins/datatables.net-responsive-bs5/css/responsive.bootstrap5.min.css" rel="stylesheet" />
<link href="/assets/plugins/bootstrap-table/dist/bootstrap-table.min.css" rel="stylesheet" />
<!-- ================== END page-css ================== -->

</head>
<body>
	<!-- BEGIN #app -->
	<div id="app" class="app">
		<!-- 헤드바 -->
		{{>plugin/head_bar}}
		<!-- 사이드바 -->		
		{{>plugin/sidebar}}
		<!-- BEGIN #content -->
		<div id="content" class="app-content">
			<!-- BEGIN container -->
			<div class="container">
				<!-- BEGIN row -->
				<div class="row justify-content-center">
					<!-- BEGIN col-10 -->
					<div class="col-xl-10">
						<!-- BEGIN row -->
						<div class="row">
							<!-- BEGIN col-9 -->
							<div class="col-xl-9">
								<ul class="breadcrumb">
									<li class="breadcrumb-item">인사</li>
									<li class="breadcrumb-item active"><a href="javascript:location.reload();">{{menuName}}</a></li>
								</ul>
								<!-- BEGIN 계정 목록 -->
								<div id="bootstrapTable" class="mb-5">
									<h4>계정 목록</h4>
									<div class="card">
										<div class="card-body">
											<table class="table" data-toggle="table" data-sort-class="table-active" data-sortable="true" data-search="true" data-pagination="true" data-show-refresh="true" data-show-columns="true" data-show-fullscreen="true" data-height="460" data-click-to-select="true">
												<thead>
													<tr>
														<th data-field="id" data-checkbox="true"></th>
														<th class="text-theme" data-sortable="true">이름</th>
														<th class="text-theme" data-sortable="true">이메일</th>
														<th class="text-theme" data-sortable="true">접속상태</th>
														<th class="text-theme" data-sortable="true">계정상태</th>
														<th class="text-theme" data-sortable="true">주소</th>
														<th class="text-theme" data-sortable="true">연락처</th>
														<th class="text-theme" data-sortable="true">입사일</th>
													</tr>
												</thead>
												<tbody>
													{{#accountList}}
													<tr>
														<td>{{id}}</td>
														<td class="text-light">{{name}}</td>
														<td class="text-light">{{email}}</td>
														<td class="text-light">{{userStatus.value}}</td>
														<td class="text-light">{{status.value}}</td>
														<td class="text-light">{{address}} {{addressDetail}}</td>
														<td class="text-light">{{phone}}</td>
														<td class="text-light">{{hireDate}}</td>
													</tr>
													{{/accountList}}
													{{^accountList}}
													{{/accountList}}
												</tbody>
											</table>
										</div>
										<div class="card-arrow">
											<div class="card-arrow-top-left"></div>
											<div class="card-arrow-top-right"></div>
											<div class="card-arrow-bottom-left"></div>
											<div class="card-arrow-bottom-right"></div>
										</div>
<!-- 										<div class="hljs-container">
											<pre>
												<code>&lt;!-- required css / js --&gt;
&lt;link href="/assets/plugins/bootstrap-table/dist/bootstrap-table.min.css" rel="stylesheet" /&gt;
&lt;script src="/assets/plugins/bootstrap-table/dist/bootstrap-table.min.js"&gt;&lt;/script&gt;

&lt;!-- html --&gt;
&lt;table class="table" data-toggle="table"
  data-sort-class="table-active"
  data-sortable="true"
