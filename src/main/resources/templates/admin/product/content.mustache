<div class="container-fluid">
	<!-- DataTales -->
	<div class="card shadow mb-4">
		<div class="card-body">
			<div class="table-responsive-xl">
				<table class="table table-bordered" id="productTable" width="100%"
					cellspacing="0" style="text-align: center">
					<thead>
						<tr>
							<th></th>
							<th>등록번호</th>
							<th>유형</th>
							<th>분류</th>
							<th>상품명</th>
							<th>가격</th>
							<th>매입가</th>
							<th>제한등급</th>
							<th>재고</th>
							<th>발매일자</th>
							<th>등록일자</th>
							<th>할인율</th>
							<th>최종금액</th>
							<th>상세정보</th>
						</tr>
					</thead>
					<tbody id="tbody">
						{{#productList}}
						<tr>
							<td></td>
							<td id="productId">{{id}}</td>
							<td>{{type}}</td>
							<td>{{category}}</td>
							<td>{{name}}</td>
							<td>{{price}}</td>
							<td>{{cost}}</td>
							<td>{{rated}}</td>
							<td id="productStock">{{stock}}</td>
							<td>{{releaseDate}}</td>
							<td>{{registDate}}</td>
							<td>{{discountRate}}</td>
							<td>{{afterDiscountPrice}}</td>
							<td><button
									class="btn btn-sm btn-info shadow-sm updateModal">
									<i class="fas fa-search fa-sm text-white-100"></i>
								</button></td>
						</tr>
						{{/productList}}
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>